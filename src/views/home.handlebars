<!-- Statistics -->
<div class="counter">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">

                <!-- Counter -->

                <div class="stats-container">
                    <div class="repo-stats">
                        <h3>Strezniska aplikacija</h3>
                        <div id="counter">
                            <div class="cell">
                                <div style="color: #00c9db;" class="counter-value number-count" data-count="{{stats.api.commits}}">1</div>
                                <p class="counter-info">Commits</p>
                            </div>
                            <div class="cell">
                                <div style="color: #00c9db;" class="counter-value number-count" data-count="{{stats.api.branches}}">1</div>
                                <p class="counter-info">Branches</p>
                            </div>
                            <div class="cell">
                                <a href="{{stats.api.lastCommit.url}}" class="number-count last-commit">{{stats.api.lastCommit.sha}}</a>
                                <p class="counter-info">Last commit</p>
                            </div>
                        </div>
                    </div>



                    <div class="repo-stats">
                        <h3>Mobilna aplikacija</h3>
                        <div id="counter">
                            <div class="cell">
                                <div style="color: #00c9db;" class="counter-value number-count" data-count="{{stats.mobile.commits}}">1</div>
                                <p class="counter-info">Commits</p>
                            </div>
                            <div class="cell">
                                <div style="color: #00c9db;" class="counter-value number-count" data-count="{{stats.mobile.branches}}">1</div>
                                <p class="counter-info">Branches</p>
                            </div>
                            <div class="cell">
                                <a href="{{stats.mobile.lastCommit.url}}" class="number-count last-commit">{{stats.mobile.lastCommit.sha}}</a>
                                <p class="counter-info">Last commit</p>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- end of counter -->

            </div> <!-- end of col -->
        </div> <!-- end of row -->
    </div> <!-- end of container -->
</div> <!-- end of counter -->
<!-- end of statistics -->


<!-- Contact -->
<div id="subscription" class="form">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2 id="subscription-title">NAROCI SE NA NOVICE</h2>
                <ul class="list-unstyled li-space-lg">
                    <li class="address">Obvestili te bomo ko bo aplikacija na voljo.</li>
                </ul>
            </div> <!-- end of col -->
        </div> <!-- end of row -->
        <div class="row">
            <div class="col-lg-6 offset-lg-3">

                <!-- Contact Form -->
                <form id="contactForm" data-toggle="validator" data-focus="false">
                    <div class="form-group">
                        <select id="school-select" required>
                            <option selected disabled>Izberi solo</option>
                            <option value="volvo">SCNG</option>
                            <option value="saab">ERS</option>
                            <option value="opel">VSS</option>
                            <option value="audi">SPLS</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <input type="email" class="form-control-input" id="cemail" required>
                        <label class="label-control" for="cemail">Email</label>
                        <div class="help-block with-errors"></div>
                    </div>
                    <!--div class="form-group checkbox">
                        <input type="checkbox" id="cterms" value="Agreed-to-Terms" required>I have read and agree to Leno's stated conditions in <a href="privacy-policy.html">Privacy Policy</a> and <a href="terms-conditions.html">Terms Conditions</a>
                        <div class="help-block with-errors"></div>
                    </div-->
                    <div class="form-group">
                        <button id="submit-button" class="form-control-submit-button">OBVESTI ME</button>
                    </div>
                    <div class="form-message">
                        <div id="cmsgSubmit" class="h3 text-center hidden"></div>
                    </div>
                </form>
                <!-- end of contact form -->

            </div> <!-- end of col -->
        </div> <!-- end of row -->
    </div> <!-- end of container -->
</div> <!-- end of form -->
